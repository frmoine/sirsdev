<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import org.geotoolkit.gui.javafx.util.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root maxWidth="1.7976931348623157E308" type="BorderPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <HBox maxWidth="1.7976931348623157E308" spacing="10.0" styleClass="gray-light" BorderPane.alignment="CENTER">
         <children>
            <HBox styleClass="geobutton">
               <children>
                  <Label maxHeight="1.7976931348623157E308" styleClass="label-header" text="Période de validité">
                     <padding>
                        <Insets bottom="5.0" left="10.0" right="15.0" top="5.0" />
                     </padding>
                  </Label>
               </children>
            </HBox>
            <HBox alignment="CENTER" maxWidth="1.7976931348623157E308" spacing="10.0" styleClass="gray-light" HBox.hgrow="ALWAYS">
               <children>
                  <Separator maxWidth="1.7976931348623157E308" orientation="VERTICAL" visible="false" />
                  <Label text="Début :">
                     <padding>
                        <Insets left="10.0" />
                     </padding>
                  </Label>
                  <DatePicker fx:id="uiDateDebut" />
                  <Separator maxWidth="1.7976931348623157E308" orientation="VERTICAL" visible="false" HBox.hgrow="ALWAYS" />
                  <Label text="Fin :">
                     <padding>
                        <Insets left="10.0" />
                     </padding>
                  </Label>
                  <DatePicker fx:id="uiDateFin" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </HBox>
         </children>
      </HBox>
   </center>
</fx:root>
